.city-container {
	width: 100%;
	position: relative;
	top: 45px;
	left: 0;
	background-color: #ffffff;
	z-index: 9999;
	display: none;
}
.letter {
	width: 10%;
	overflow: hidden;
	line-height: 30px;
	font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size: 70.25%;
	position: absolute;
	top: 0px;
	right: 0px;
	text-align: center;
	background-color: #DF6659;
}
.letter ul {
	list-style-type: none;
}
.letter ul li a {
	text-decoration: none;
}
.letter ul li a:link {
	text-decoration: none;
	color: #404D5B;
}
.city {
	width: 90%;
	overflow: hidden;
}
.city-list {
	width: 100%;
	overflow: hidden;
}
.city-list .city-letter {
	color: #6e6e6e;
	font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size: 70.25%;
	display: inline-block;
	border-bottom: 1px solid #e8ecf1;
	width: 100%;
}
.city-list p {
	color: #afafaf;
	width: 100%;
	height: 30px;
	line-height: 30px;
	border-bottom: 1px solid #e8ecf1;
	cursor: pointer;
	margin: 10 auto;
	padding: 0.1em;
}
.UISelect {
	margin-top: 5px
}
.ac-custom {
	padding: 0 3em;
	max-width: 900px;
	margin: 0 auto;
	display: inline;
	outline:none;
	
}
.ac-custom h2 {
	font-size: 3em;
	font-weight: 300;
	padding: 0 0 0.5em;
	margin: 0 0 30px;
	outline:none;
}
.ac-custom ul, .ac-custom ol {
	list-style: none;
	padding: 0;
	margin: 0 auto;
	max-width: 600px;
	outline:none;
}
.ac-custom li {
	width: 100px;
	text-align: left;
	margin-left: 10px;
	padding: 2em 0;
	position: relative;
	display: inline-block;
	outline:none;
}
.ac-custom label {
	margin-left: 30px;
	position: relative;
	font-size: 70.25%;
	font-weight: bold;
	padding: 0 0 0 50px;
	vertical-align: top;
	outline:none;
	color: rgba(255,255,255,0.8);
	cursor: pointer;
	-webkit-transition: color 0s;
	transition: color 0s;
}
.ac-custom input[type="checkbox"], .ac-custom input[type="radio"], .ac-custom label::before {
	width: 40px;
	height: 40px;
	top: 50%;
	left: 0;
	margin-top: -25px;
	position: absolute;
	cursor: pointer;
	outline:none;
}
.ac-custom input[type="checkbox"], .ac-custom input[type="radio"] {
	opacity: 0;
	-webkit-appearance: none;
	display: inline-block;
	vertical-align: middle;
	z-index: 100;
	outline:none;
}
.ac-custom label::before {
	content: '';
	border: 4px solid #F0EFEE;
	-webkit-transition: opacity 0s;
	transition: opacity 0s;
	outline:none;
}
.ac-radio label::before {
	border-radius: 50%;
}
.ac-custom input[type="checkbox"]:checked + label, .ac-custom input[type="radio"]:checked + label {
	color: #DF6659;
	outline:none;
}
.ac-custom input[type="checkbox"]:checked + label::before, .ac-custom input[type="radio"]:checked + label::before {
	border: 4px solid #DF6659;
	outline:none;
}
