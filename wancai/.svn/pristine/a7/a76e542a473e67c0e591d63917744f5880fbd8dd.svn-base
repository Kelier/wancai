<!doctype html>
<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
		<title>载入</title>
		<link rel="stylesheet" type="text/css" href="css/aui/aui.css" />
		
	</head>
	<body></body>
	<script type="text/javascript" src="script/api.js"></script>
	<script type="text/javascript">
		apiready = function() {
			api.parseTapmode();
			//设置APIKEY
			var model = api.require('model');
			model.config({
				appId : 'A6910011535894',
				appKey : '114BA070-B206-49B2-17E7-AD685B40A11F',
				host : 'https://d.apicloud.com'
			});

			var showGuide = $api.getStorage('showGuide');
			if (!showGuide) {
				openGuide();
			} else {
				openLogin();
			}
			//	checkUserId();
		}
		//打开引导页
		function openGuide() {
			api.openFrame({
				name : 'guide',
				url : 'launch_guide.html',
				bounces : false,
				rect : {
					x : 0,
					y : 0,
					w : 'auto',
					h : 'auto'
				}
			});
		}
	
		//打开登录页
		function openLogin() {
			api.openFrame({
				name : 'login',
				url : 'launch_guide.html',//html/login_System/login.html',     ./html/person_System/person_win2.html
				bounces : false,
				reload: true,
				rect : {
					x : 0,
					y : 0,
					w : 'auto',
					h : 'auto'
				}
			});
		}

		function checkUserId() {
			if (localStorage.getItem('userId')) {
				/*
				 api.openFrame({
				 name : "total_win",
				 url : './html/total_win.html',
				 rect : {
				 x : 0,
				 y : 0,
				 w : "auto",
				 h : 'auto'
				 }
				 })
				 */
				api.openFrame({
					name : "login",
					url : 'login2.html',
					rect : {
						x : 0,
						y : 0,
						w : "auto",
						h : 'auto'
					}
				})
			} else {
				api.openFrame({
					name : "welcome",
					url : './html/slider_up_frm.html',
					rect : {
						x : 0,
						y : 0,
						w : "auto",
						h : 'auto'
					}
				})
			}
			
			
		}
	</script>
</html>